{
  "version": 8,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 29,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "baremetal-arm",
      "hidden": true,
      "cacheVariables": {
        "PLATFORM": "baremetal-arm",
        "BAREMETAL_ARM_TOOLCHAIN_PATH": "/opt/toolchains/gcc-arm-11.2-2022.02-x86_64-arm-none-eabi",
        "APP_BAREMETAL_FLAGS": "-mcpu=cortex-m4 -mfloat-abi=hard -mfpu=fpv4-sp-d16 -mthumb",
        "APP_C_FLAGS": "${APP_BAREMETAL_FLAGS}",
        "APP_CXX_FLAGS": "${APP_BAREMETAL_FLAGS}"
      }
    },
    {
      "name": "freertos-arm",
      "hidden": true,
      "cacheVariables": {
        "PLATFORM": "freertos-arm",
        "BAREMETAL_ARM_TOOLCHAIN_PATH": "/opt/toolchains/gcc-arm-11.2-2022.02-x86_64-arm-none-eabi",
        "FREERTOS_VERSION": "freertos-10.2.1",
        "FREERTOS_PORTABLE": "ARM_CM4F",
        "APP_BAREMETAL_FLAGS": "-mcpu=cortex-m4 -mfloat-abi=hard -mfpu=fpv4-sp-d16 -mthumb"
      }
    }
  ]
}
