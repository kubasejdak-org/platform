{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 25,
    "patch": 2
  },
  "include": [
    "cmake/presets-baremetal.json",
    "cmake/presets-linux.json",
    "cmake/presets-type.json"
  ],
  "configurePresets": [
    {
      "name": "linux-gcc-debug",
      "inherits": [
        "linux-gcc",
        "debug"
      ]
    },
    {
      "name": "linux-gcc-release",
      "inherits": [
        "linux-gcc",
        "release"
      ]
    },
    {
      "name": "linux-clang-debug",
      "inherits": [
        "linux-clang",
        "debug"
      ]
    },
    {
      "name": "linux-clang-release",
      "inherits": [
        "linux-clang",
        "release"
      ]
    },
    {
      "name": "linux-arm-gcc-debug",
      "inherits": [
        "linux-arm-gcc",
        "debug"
      ]
    },
    {
      "name": "linux-arm-gcc-release",
      "inherits": [
        "linux-arm-gcc",
        "release"
      ]
    },
    {
      "name": "linux-arm-clang-debug",
      "inherits": [
        "linux-arm-clang",
        "debug"
      ]
    },
    {
      "name": "linux-arm-clang-release",
      "inherits": [
        "linux-arm-clang",
        "release"
      ]
    },
    {
      "name": "baremetal-arm-debug",
      "inherits": [
        "baremetal-arm",
        "debug"
      ]
    },
    {
      "name": "baremetal-arm-release",
      "inherits": [
        "baremetal-arm",
        "release"
      ]
    },
    {
      "name": "freertos-arm-debug",
      "inherits": [
        "freertos-arm",
        "debug"
      ]
    },
    {
      "name": "freertos-arm-release",
      "inherits": [
        "freertos-arm",
        "release"
      ]
    }
  ]
}
